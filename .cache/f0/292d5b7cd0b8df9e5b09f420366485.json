box;\n  transition: all 0.2s ease-in-out;\n}\n\nhtml, body {\n  width: 100%;\n  height: 100vh;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: url(\"./images/pattern-background-desktop.svg\");\n  background-size: contain;\n  background-repeat: no-repeat;\n  background-color: var(--primary);\n}\n\nh1, p, h2, a, span, button {\n  font-family: var(--font);\n  margin: 0;\n}\nh1 {\n  font-size: 1.5rem;\n  color: var(--dark);\n  font-weight: 900;\n}\nh2 {\n  font-size: 1rem;\n  font-weight: 900;\n  color: var(--dark);\n}\np {\n  font-size: 1rem;\n  line-height: 1.4;\n  font-weight: 500;\n  color: var(--medium);\n  text-align: center;\n}\na {\n  color: var(--secondary);\n  font-size: 0.8rem;\n  font-weight: 700;\n}\na:hover {\n  color: hsla(245, 75%, 52%, 0.75);\n}\nbutton {\n  cursor: pointer;\n  width: 100%;\n  height: 50px;\n  font-weight: 900;\n  border-radius: 10px;\n  text-align: center;\n  border: none;\n}\nbutton.accept {\n  background: var(--secondary);\n  color: var(--white);\n  box-shadow: 0 10px 20px -10px var(--secondary);\n}\nbutton.accept:hover {\n  background: hsla(245, 75%, 52%, 0.75);\n}\nbutton.reject {\n  background: transparent;\n  color: var(--medium);\n}\nbutton.reject:hover {\n  color: var(--dark);\n}\n\nmain {\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  width: 320px;\n  height: 552px;\n  border-radius: 20px;\n  background: var(--white);\n  overflow: hidden;\n}\nmain .header-img {\n  width: 100%;\n}\nmain .main-container {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  flex: 1 0 auto;\n  padding: 40px 20px 20px;\n}\n.order-container {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 18px;\n  border-radius: 10px;\n  background-color: var(--light);\n}\n.order-container .music-icon {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  background: var(--primary);\n  margin-right: 18px;\n}\n.order-container .plan-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: flex-start;\n  flex: 1 0 auto;\n}\n.buttons-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: flex-start;\n  width: 100%;\n  height: 110px;\n}\n\n.attribution {\n  position: absolute;\n  width: 100%;\n  bottom: 5px;\n  left: 0;\n  font-family: var(--font);\n  color: var(--dark);\n  text-align: center;\n  font-size: 1rem;\n}\n.attribution a {\n  text-decoration: none;\n  font-weight: 700;\n  color: var(--secondary);\n}\n\n@media screen and (min-width: 768px) {\n  main {\n    width: 400px;\n    height: 630px;\n  }\n  main .main-container {\n    padding: 40px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"830a4c90af41552db9754527734d7862","cacheData":{"env":{}}}